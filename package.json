{
  "name": "@antlur/backstage",
  "author": "Anthony Holmes",
  "version": "1.10.2",
  "description": "A simple client for Backstage CMS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/types/*.d.ts"
    },
    "./studio": {
      "import": "./dist/studio/index.js",
      "types": "./dist/types/studio/*.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/types/config.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build && npm test",
    "dev": "tsc --watch",
    "cli": "vite-node src/cli/cli.ts",
    "postbuild": "chmod +x ./dist/cli/cli.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "bin": {
    "backstage": "./dist/cli/cli.js"
  },
  "peerDependencies": {
    "luxon": "^3.5.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "schema-dts": "^1.1.2",
    "vite-node": "^3.0.5"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "luxon": "^3.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "src",
    "readme.md"
  ]
}
