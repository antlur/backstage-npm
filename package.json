{
  "name": "@antlur/backstage",
  "author": "Anthony Holmes",
  "version": "1.2.2",
  "description": "A simple client for Backstage CMS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types": {
      "require": "./dist/cjs/types/index.js",
      "import": "./dist/esm/types/index.js",
      "types": "./dist/types/types/*.d.ts"
    },
    "./studio": {
      "require": "./dist/cjs/studio/index.js",
      "import": "./dist/esm/studio/index.js",
      "types": "./dist/types/studio/*.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "prepublishOnly": "npm run build && npm test",
    "watch": "npm run watch:cjs & npm run watch:esm",
    "watch:cjs": "tsc --project tsconfig.cjs.json --watch",
    "watch:esm": "tsc --project tsconfig.esm.json --watch",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "peerDependencies": {
    "luxon": "^3.5.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "luxon": "^3.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "src",
    "readme.md"
  ]
}
